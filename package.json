{
  "name": "next-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nodemon",
    "build": "next build && tsc --project tsconfig.server.json",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write .",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "test": "jest",
    "test-all": "yarn lint && yarn type-check && yarn test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run type-check"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn lint",
      "yarn format"
    ]
  },
  "dependencies": {
    "next": "^10.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@hookform/resolvers": "^1.3.6",
    "@reduxjs/toolkit": "^1.5.1",
    "@tailwindcss/forms": "^0.3.2",
    "@testing-library/react": "^11.2.5",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.35",
    "@types/react": "^17.0.3",
    "@types/react-redux": "^7.1.16",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "autoprefixer": "^10.0.4",
    "axios": "^0.21.1",
    "babel-jest": "^26.6.3",
    "cors": "2.8.5",
    "cross-env": "^7.0.2",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-react": "^7.19.0",
    "express": "^4.17.1",
    "firebase": "^7.6.2",
    "husky": "^4.2.3",
    "identity-obj-proxy": "^3.0.0",
    "ismobilejs": "^1.1.1",
    "jest": "^26.6.3",
    "jest-watch-typeahead": "^0.6.1",
    "lint-staged": "^10.0.10",
    "localforage": "^1.7.3",
    "lodash": "^4.17.21",
    "nodemon": "^2.0.7",
    "postcss": "^8.1.10",
    "prettier": "^2.0.2",
    "react-hook-form": "^6.15.5",
    "react-redux": "^7.2.3",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "swr": "^0.5.6",
    "tailwindcss": "^2.1.1",
    "ts-node": "^8.10.2",
    "typescript": "^4.2.3",
    "yup": "^0.32.9"
  },
  "license": "MIT"
}
